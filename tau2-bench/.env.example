# Example environment configuration for tau2-bench.
# Copy to ".env" and fill in real credentials locally.
#
# IMPORTANT: Do not commit real API keys.

USE_AZURE_OPENAI="false"

# Standard OpenAI (used when USE_AZURE_OPENAI=false)
OPENAI_API_KEY=""

# Azure OpenAI (used when USE_AZURE_OPENAI=true)
AZURE_OPENAI_ENDPOINT=""
AZURE_OPENAI_API_VERSION="2025-01-01-preview"
AZURE_OPENAI_DEPLOYMENT="gpt-4.1"
AZURE_OPENAI_MODELS="gpt-4.1"

# Optional request logging
LLM_LOG_ENABLED="true"
LLM_LOG_DIR="logs/llm_requests"

